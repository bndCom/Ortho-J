<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orthoj.Controller.ShowEpreuveTestsExercices">
   <children>
      <Label fx:id="nomBO" layoutX="38.0" layoutY="24.0" text="nom">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox layoutX="100.0" layoutY="51.0" prefHeight="25.0" prefWidth="401.0" spacing="10.0">
         <children>
            <Button fx:id="anamnese" mnemonicParsing="false" onAction="#onAnamnese" text="anamnese" />
            <Button fx:id="epreuve" mnemonicParsing="false" onAction="#onEpreuve" text="epreuve clinique" />
            <Button fx:id="diagnostic" mnemonicParsing="false" onAction="#onDiagnostic" text="diagnostic" />
            <Button fx:id="projet" mnemonicParsing="false" onAction="#onProjet" text="projet thérapeutique" />
         </children>
      </HBox>
      <TableView fx:id="exercicesTable" layoutX="52.0" layoutY="119.0" prefHeight="200.0" prefWidth="498.0">
        <columns>
          <TableColumn fx:id="nomColumn" prefWidth="201.0" text="Nom" />
          <TableColumn fx:id="materielColumn" minWidth="0.0" prefWidth="209.0" text="Matériel" />
            <TableColumn fx:id="scoreColumn" prefWidth="87.0" text="Score" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
