<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orthoj.Controller.AnamAdulteHomeHistoir">
    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="stringColumn" text="String" />
        </columns>
    </TableView>
    <HBox alignment="CENTER" spacing="10">
        <TextField fx:id="inputField" promptText="Enter new string" />
        <Button onAction="#handleAdd" text="Add" />
    </HBox>
    <HBox alignment="CENTER" spacing="10">
        <Button onAction="#handleEdit" text="Edit" />
        <Button onAction="#handleDelete" text="Delete" />
    </HBox>
</VBox>
