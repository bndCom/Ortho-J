<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orthoj.Controller.ShowAnam">
   <children>
      <Label fx:id="nomBO" layoutX="38.0" layoutY="24.0" text="nom">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox layoutX="100.0" layoutY="51.0" prefHeight="25.0" prefWidth="401.0" spacing="10.0">
         <children>
            <Button fx:id="anamnese" mnemonicParsing="false" onAction="#onAnamnese" text="anamnese" />
            <Button fx:id="epreuve" mnemonicParsing="false" onAction="#onEpreuve" text="epreuve clinique" />
            <Button fx:id="diagnostic" mnemonicParsing="false" onAction="#onDiagnostic" text="diagnostic" />
            <Button fx:id="projet" mnemonicParsing="false" onAction="#onProjet" text="projet thérapeutique" />
         </children>
      </HBox>
      <TableView fx:id="anamTable" layoutX="46.0" layoutY="118.0" prefHeight="167.0" prefWidth="494.0">
         <columns>
            <TableColumn fx:id="categorieColumn" prefWidth="141.0" text="Catégorie" />
            <TableColumn fx:id="questionColumn" prefWidth="184.0" text="Question" />
            <TableColumn fx:id="reponseColumn" prefWidth="168.0" text="Réponse" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
