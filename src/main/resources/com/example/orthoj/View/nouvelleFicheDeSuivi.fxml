<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orthoj.Controller.NouvelleFicheDeSuiviController">

    <VBox />
  <TableView fx:id="tableView1" layoutX="313.0" layoutY="47.0" prefHeight="322.0" prefWidth="248.0">
      <columns>
          <TableColumn fx:id="nameColumn" text="Name" />
          <TableColumn fx:id="typeColumn" text="Type" />
      </columns>
  </TableView>
  <TextField fx:id="inputField1" layoutX="32.0" layoutY="89.0" promptText="Enter an objectif" />
  <ComboBox fx:id="typeComboBox" layoutX="204.0" layoutY="89.0" />
  <Button layoutX="32.0" layoutY="135.0" onAction="#handleAddObjectif" text="Add Objectif" />
   <Button layoutX="60.0" layoutY="196.0" mnemonicParsing="false" onAction="#save" text="sauvegarder" />
   <Button layoutX="178.0" layoutY="196.0" mnemonicParsing="false" onAction="#notnew" text="Skip" />
   <Button layoutX="34.0" layoutY="244.0" mnemonicParsing="false" onAction="#onMenu" text="Menu" />
</AnchorPane>
