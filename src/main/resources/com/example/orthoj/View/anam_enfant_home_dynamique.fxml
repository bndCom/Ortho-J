<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orthoj.Controller.AnamEnfantHomeDynamique">
   <children>
      <VBox layoutX="14.0" layoutY="98.0" prefHeight="205.0" prefWidth="156.0" spacing="5.0">
         <children>
            <Button fx:id="structure" mnemonicParsing="false" onAction="#onStructure" prefHeight="25.0" prefWidth="178.0" text="Structure familiale" />
            <Button fx:id="dynamique" mnemonicParsing="false" onAction="#onDynamique" prefHeight="25.0" prefWidth="185.0" text="Dynamique familiale" />
            <Button fx:id="antecedent" mnemonicParsing="false" onAction="#onAntecedent" prefHeight="25.0" prefWidth="183.0" text="Antecedents familiaux" />
            <Button fx:id="condition" mnemonicParsing="false" onAction="#onCondition" prefHeight="25.0" prefWidth="192.0" text="Conditions natales" />
            <Button fx:id="psycho" mnemonicParsing="false" onAction="#onPsycho" prefHeight="25.0" prefWidth="182.0" text="Developpement psycho" />
            <Button fx:id="langagier" mnemonicParsing="false" onAction="#onLangagier" prefHeight="25.0" prefWidth="167.0" text="Developpement langagier" />
            <Button fx:id="caractere" mnemonicParsing="false" onAction="#onCaractere" prefHeight="25.0" prefWidth="159.0" text="Caractere comportement" />
         </children>
      </VBox>
      <TableView fx:id="tableView" layoutX="175.0" layoutY="60.0" prefHeight="225.0" prefWidth="411.0">
        <columns>
          <TableColumn fx:id="stringColumn" prefWidth="410.0" text="Questions" />
        </columns>
      </TableView>
      <TextField fx:id="inputField" layoutX="185.0" layoutY="303.0" prefHeight="25.0" prefWidth="177.0" />
      <Button layoutX="371.0" layoutY="303.0" mnemonicParsing="false" onAction="#handleAdd" text="Ajouter" />
      <Button layoutX="434.0" layoutY="303.0" mnemonicParsing="false" onAction="#handleEdit" text="Modifier" />
      <Button layoutX="504.0" layoutY="303.0" mnemonicParsing="false" onAction="#handleDelete" text="Supprimer" />
      <Button layoutX="509.0" layoutY="343.0" mnemonicParsing="false" onAction="#handleSave" text="Sauvegarder" />
   </children>
</AnchorPane>
