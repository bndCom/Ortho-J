<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orthoj.Controller.PatientProfile">
   <children>
      <HBox layoutX="60.0" layoutY="31.0" prefHeight="25.0" prefWidth="481.0" spacing="30.0">
         <children>
            <Button fx:id="information" mnemonicParsing="false" onAction="#onInformation" text="Information" />
            <Button fx:id="bo" mnemonicParsing="false" onAction="#onBo" text="BO" />
            <Button fx:id="fiche" mnemonicParsing="false" onAction="#onFiche" text="Fiche de suivi" />
            <Button fx:id="rdv" mnemonicParsing="false" onAction="#onRdv" text="Rendez-vous" />
            <Button fx:id="progression" mnemonicParsing="false" onAction="#onProgression" text="Progression" />
         </children>
      </HBox>
   </children>
</AnchorPane>
